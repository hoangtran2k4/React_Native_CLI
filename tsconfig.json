{
  "compilerOptions": {
    "jsx": "react-native",
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "isolatedModules": true
  },
  "include": [
    "App/**/*",
    "index.ts",
    "index.tsx",
    "declarations.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
